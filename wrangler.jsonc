{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "homepage",
  "compatibility_date": "2025-07-26",
  "workers_dev": false,
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "vars": {
    "NODE_VERSION": "24"
  },
  "routes": [
    {
      "pattern": "wolf-yuan.dev",
      "custom_domain": true
    }
  ]
}
